{
  "name": "docskit",
  "version": "1.0.0",
  "author": "@pheralb_",
  "description": "Create & share markdown posts with Supabase.",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./src/styles/globals.css -o src/styles/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./src/styles/globals.css -o src/styles/tailwind.css",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@headlessui/react": "1.7.7",
    "@monaco-editor/react": "4.4.6",
    "@remix-run/node": "1.8.2",
    "@remix-run/react": "1.8.2",
    "@remix-run/vercel": "1.8.2",
    "@supabase/auth-helpers-remix": "0.1.3",
    "@supabase/supabase-js": "2.1.3",
    "@uiball/loaders": "1.2.6",
    "@vercel/node": "2.8.0",
    "framer-motion": "7.8.0",
    "nanoid": "4.0.0",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.41.0",
    "react-hot-toast": "2.4.0",
    "react-icons": "4.7.1",
    "react-markdown": "8.0.4",
    "react-rough-notation": "1.0.3",
    "rehype-prism-plus": "1.5.0",
    "remark-gfm": "3.0.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.8.2",
    "@remix-run/eslint-config": "1.8.2",
    "@remix-run/serve": "1.8.2",
    "@tailwindcss/typography": "0.5.8",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "eslint": "8.29.0",
    "postcss": "8.4.19",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
